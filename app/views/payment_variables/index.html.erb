<h1>Listing payment_variables</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @payment_variables.each do |payment_variable| %>
  <tr>
    <td><%= payment_variable.key %></td>
    <td><%= payment_variable.value %></td>
    <td><%= link_to 'Show', payment_variable %></td>
    <td><%= link_to 'Edit', edit_payment_variable_path(payment_variable) %></td>
    <td><%= link_to 'Destroy', payment_variable, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

