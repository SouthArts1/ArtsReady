<div class="grid_8">
  <section>
    <%= form_for(current_org) do |f| %>
    <p>
      <label>Organization Name</label>
      <%= f.text_field :name %>
    </p>

    <p>
      <label>Address</label>
      <%= f.text_field :address %><br />
      <%#= f.text_field :address_2 %>
    </p>

    <p>
      <label>City</label>
      <%= f.text_field :city %>
    </p>
  <div class="grid_4 alpha">
    <p>
      <label>State</label>
      <%= f.text_field :state %>
    </p>
  </div>
  <div class="grid_4 omega">
    <p>
      <label>Zip</label>
      <%= f.text_field :zipcode %>
    </p>
  </div>
    <p>
      <label>Website</label>
      <%#= f.text_field :website %>
    </p>

    <p>
      <label>Phone</label>
      <%#= f.text_field :phone %>
    </p>

    <p>
      <label>Fax</label>
      <%#= f.text_field :fax %>
    </p>

    <p>
      <label>Join Battle Buddy Network?</label>
      <%= f.check_box :battle_buddy_enabled %>
    </p>

    <p>
      <%= f.submit 'Save Settings' %>
    </p>
    <% end %>
  </section>
</div>